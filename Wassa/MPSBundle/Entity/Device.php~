<?php

namespace Wassa\MPSBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

define('DEVICETYPE_IOS', 1);
define('DEVICETYPE_ANDROID', 1);

/**
 * Device
 *
 * @ORM\Entity
 * @ORM\Table(name="device")
 * @ORM\InheritanceType("SINGLE_TABLE")
 */
class Device
{
    /**
     * @var integer
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    protected $id;

    /**
     * @var \DateTime
     *
     * @ORM\Column(name="last_registration", type="datetime", nullable=true)
     */
    protected $lastRegistration;

    /**
     * @var array
     *
     * @ORM\Column(name="custom_data", type="json_array", nullable=true)
     */
    private $customData;

    /**
     * @var MPSUser
     *
     * @ORM\OneToOne(targetEntity="MPSUser", mappedBy="device", cascade={"persist"})
     */
    private $user;

    /**
     * @var Platform
     *
     * @ORM\OneToOne(targetEntity="Platform")
     */
    private $platform;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set lastRegistration
     *
     * @param \DateTime $lastRegistration
     * @return Device
     */
    public function setLastRegistration($lastRegistration)
    {
        $this->lastRegistration = $lastRegistration;

        return $this;
    }

    /**
     * Get lastRegistration
     *
     * @return \DateTime 
     */
    public function getLastRegistration()
    {
        return $this->lastRegistration;
    }

    /**
     * Set customData
     *
     * @param array $customData
     * @return Device
     */
    public function setCustomData($customData)
    {
        $this->customData = $customData;

        return $this;
    }

    /**
     * Get customData
     *
     * @return array
     */
    public function getCustomData()
    {
        return $this->customData;
    }

    /**
     * Set user
     *
     * @param MPSUser $user
     * @return Device
     */
    public function setUser($user)
    {
        $this->user = $user;

        return $this;
    }

    /**
     * Get user
     *
     * @return MPSUser
     */
    public function getUser()
    {
        return $this->user;
    }

    /**
     * Set platform
     *
     * @param \Wassa\MPSBundle\Entity\Platform $platform
     * @return Device
     */
    public function setPlatform(\Wassa\MPSBundle\Entity\Platform $platform = null)
    {
        $this->platform = $platform;
    
        return $this;
    }

    /**
     * Get platform
     *
     * @return \Wassa\MPSBundle\Entity\Platform 
     */
    public function getPlatform()
    {
        return $this->platform;
    }
}